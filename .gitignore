# The directory Mix will write compiled artifacts to.
/_build/

# If you run "mix test --cover", coverage assets end up here.
/cover/

# The directory Mix downloads your dependencies sources to.
/deps/

# Where 3rd-party dependencies like ExDoc output generated docs.
/doc/

# Ignore .fetch files in case you like to edit your project deps locally.
/.fetch

# If the VM crashes, it generates a dump, let's ignore it too.
erl_crash.dump

# Ignore archive artifacts (built via "mix archive.build").
*.ez

# Temporary files, for example, from tests.
/tmp/

# Ignore package tarball (built via "mix hex.build").
droodotfoo-*.tar

# Ignore assets that are produced by build tools.
/priv/static/assets/

# Ignore digested assets cache.
/priv/static/cache_manifest.json

# Ignore digested astro files (generated by phx.digest)
/priv/static/astro/*-*.js
/priv/static/astro/*-*.js.gz
/priv/static/astro/*.js.br
/priv/static/astro/*.js-*.br

# Ignore digested versions of static files (generated by phx.digest)
/priv/static/llms.txt.gz
/priv/static/llms.txt.br
/priv/static/llms.txt-*.br
/priv/static/llms-*.txt
/priv/static/llms-*.txt.gz
/priv/static/llms-*.txt.br
/priv/static/robots.txt.gz
/priv/static/robots.txt.br
/priv/static/robots.txt-*.br
/priv/static/robots-*.txt
/priv/static/robots-*.txt.gz
/priv/static/robots-*.txt.br

# Ignore .well-known digested files
/priv/static/.well-known/*.br
/priv/static/.well-known/*.gz

# Ignore all digested static assets (hash in filename)
/priv/static/fonts/*-*.woff2
/priv/static/images/*-*.png
/priv/static/images/*-*.jpg
/priv/static/images/*-*.svg
/priv/static/images/*-*.svg.gz
/priv/static/images/*-*.ico
/priv/static/images/blog/*-*.jpg
/priv/static/images/blog/*-*.webp
/priv/static/models/*-*.stl
/priv/static/favicon-*.ico
/priv/static/manifest-*.json
/priv/static/manifest-*.json.gz
/priv/static/manifest.json.gz

# Ignore brotli compressed images and icons
/priv/static/images/**/*.br

# In case you use Node.js/npm, you want to ignore these.
npm-debug.log
/assets/node_modules/
/assets/gleam/build/
/node_modules/
/test/accessibility/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json

# Ignore MacOS files
.DS_Store

# Ignore editor/IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Ignore environment variables
.env
.env.local
.env.*.local

# Ignore Elixir LS (Language Server)
.elixir_ls/

# Ignore dialyzer PLT files
priv/plts/

# Ignore test coverage reports
coverage/

# Ignore local configuration
config/*.secret.exs

# Ignore ASCII art files and documentation with visual formatting
*.ascii
*.art
*_ascii.txt
*_art.txt

# Ignore generated documentation with ASCII art
docs/ascii/
docs/art/

# Claude
.claude/
